{
  "extends": "tslint-config-airbnb-base",
  "rules": {
    "brace-style": [
      "error",
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "ter-arrow-body-style": ["error", "as-needed"],
    "no-magic-numbers": false,
    "ter-max-len": [
      "warn",
      200
    ]
  }
}
